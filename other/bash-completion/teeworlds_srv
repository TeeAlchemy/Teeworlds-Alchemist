_teeworlds_srv_commands_helper() {
	local cur="$1"
	local configs="console_output_level dbg_hitch dbg_pref dbg_stress dbg_stress_network debug logfile_timestamp net_tcp_abort_on_close sv_auto_demo_max sv_auto_demo_record sv_external_port sv_high_bandwidth sv_map_download_speed sv_max_clients sv_max_clients_per_ip sv_port sv_rcon_bantime sv_rcon_max_tries sv_register bindaddr logfile password sv_hostname sv_map sv_name sv_rcon_mod_password sv_rcon_password dbg_dummies sv_countdown sv_inactivekick sv_inactivekick_spec sv_inactivekick_time sv_match_swap sv_matches_per_map sv_player_ready_mode sv_player_slots sv_powerups sv_respawn_delay_tdm sv_scorelimit sv_silent_spectator_mode sv_skill_level sv_spamprotection sv_strict_spectate_mode sv_teambalance_time sv_teamdamage sv_timelimit sv_tournament_mode sv_vote_kick sv_vote_kick_bantime sv_vote_kick_min sv_vote_spectate sv_vote_spectate_rejoindelay sv_warmup sv_gametype sv_maprotation sv_motd ec_auth_timeout ec_bantime ec_output_level ec_port net_tcp_abort_on_close ec_bindaddr ec_password"
	local commands="add_vote ban ban bans bans_save broadcast change_map clear_votes dbg_lognetwork echo eval_if exec force_teambalance kick lock_teams logout mod_command mod_status pause record reload remove_vote restart save_config say set_team set_team_all shuffle_teams shutdown status stoprecord swap_teams toggle tune tune_reset tunes unban unban_all vote"
	COMPREPLY+=($(compgen -W "$configs" -- "$cur"))
	COMPREPLY+=($(compgen -W "$commands" -- "$cur"))
}
# generated end

_teeworlds_srv() {
	COMPREPLY=()
	local cur="${COMP_WORDS[COMP_CWORD]}"
	local commands="-s --silent -d --default -f"
	local gametypes="dm tdm ctf lms lts"
	if [[ "$COMP_CWORD" -gt "1" ]]
	then
		local lastarg="${COMP_WORDS[$COMP_CWORD-1]}"
		case "$lastarg" in
			-f)
				_filedir
				;;
			'exec'|logfile)
				_filedir
				;;
			sv_gametype)
				COMPREPLY+=($(compgen -W "${gametypes}" -- "${cur}"))
				;;
			*)
				COMPREPLY+=($(compgen -W "${commands}" -- "${cur}"))
				_teeworlds_srv_commands_helper "$cur"
				;;
		esac
	else
		COMPREPLY+=($(compgen -W "${commands}" -- "${cur}"))
		_teeworlds_srv_commands_helper "$cur"
	fi
}

complete -F _teeworlds_srv teeworlds_srv

